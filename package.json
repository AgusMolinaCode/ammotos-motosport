{
  "name": "turn14_ammotos",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "db:start": "docker-compose up -d",
    "db:stop": "docker-compose down",
    "db:logs": "docker-compose logs -f postgres",
    "db:shell": "docker exec -it turn14-postgres psql -U turn14user -d turn14db",
    "db:studio": "dotenv -e .env.local -- npx prisma studio",
    "prisma:generate": "dotenv -e .env.local -- npx prisma generate",
    "prisma:push": "dotenv -e .env.local -- npx prisma db push",
    "test:env": "dotenv -e .env.local -- tsx scripts/test-env.ts",
    "test:auth": "dotenv -e .env.local -- tsx scripts/test-auth.ts"
  },
  "dependencies": {
    "@material-tailwind/react": "^2.1.10",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@tabler/icons-react": "^3.36.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.562.0",
    "motion": "^12.24.10",
    "next": "16.1.1",
    "pg": "^8.16.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20.19.27",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^16.6.1",
    "dotenv-cli": "^7.4.4",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "prisma": "^7.2.0",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
